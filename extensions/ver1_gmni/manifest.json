{
    "manifest_version": 3,
    "name": "My Premium Blog Access",
    "version": "1.0",
    "description": "Unlocks premium blog posts for users who have the extension installed.",
    "background": {
        "service_worker": "background.js"
    },
    "externally_connectable": {
        "matches": [
            "http://127.0.0.1:5500/extensions/ver1_gmni_web/*"
        ]
    },
    "icons": {
        "16": "icons/16px.png",
        "48": "icons/48px.png",
        "128": "icons/128px.png"
    }
}